@import url(http://fonts.googleapis.com/css?family=Open+Sans);
@import url(http://fonts.googleapis.com/css?family=Lato);
*
	border 0
	margin 0
	padding 0

body {
	font-family 'Open Sans', sans-serif
	font-size 16px
}

figure
	margin 0
	padding 0
	
footer
	background #333333
	height auto
	padding 1.2em 0 1.2em 0
	width 100%
	a:hover
		text-decoration underline

h1
	margin 0
	padding 0
	
h2
	font-family 'Lato', sans-serif
	
header
	background #2da862
	height auto
	opacity 0.9
	width 100%
	
.Boton
	border-radius 5px
	border solid 1px #FFFFFF
	font-size .9em
	-webkit-border-radius 5px
	-moz-border-radius 5px
	-o-border-radius 5px
	-ms-border-radius 5px
	margin 0 0 2em 0
	padding 1em 0 1em 0
	text-align center
	width 17%
	a
		color #FFFFFF
		text-decoration none
	
.Columnas
	height auto
	margin 0 auto
	margin-bottom 100px
	margin-top 50px
	max-width 1200px
	text-align center
	width 100%
	.Col1
		display inline-block
		height auto
		margin 0 .5em 0 .5em
		vertical-align top
		width 30%
		h2
			color #27ae60
			font-size 1em
			margin 0 0 1em 0
		p
			color #505252
			font-size .9em
			line-height 23px
			margin .7em 1em .7em 1em
		a
			-webkit-transition all 0.5s ease
			-moz-transition all 0.5s ease
			-o-transition all 0.5s ease
			transition all 0.5s ease
		a:hover
			opacity 0.8
			-webkit-transform translate(0px, 10px)
			-moz-transform translate(0px, 10px)
			-o-transform translate(0px, 10px)
			-ms-transform translate(0px, 10px)
			transform translate(0px, 10px)
	.Col2
		display inline-block
		height auto
		margin 0 .5em 0 .5em
		vertical-align top
		width 30%
		a
			-webkit-transition all 0.5s ease
